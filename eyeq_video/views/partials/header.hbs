<div id='head'>  		
  	<div class="container-fluid">
  		<div class="row" id='navbar'>
  			<div class='col-xs-3' id='navbar-menu-icon'>
  				<span id='navmenu' class="glyphicon glyphicon-th"></span> 
  				
  			</div>
  			
  			<div id='navbar-title' class="col-xs-6">{{title}}</div>
  			
  			<div id='navbar-settings-icon' class="col-xs-3">
  				<span id='settings' class="glyphicon glyphicon-cog" data-content="<dl style='min-width:270px'><dt>Client Id</dt><dd>{{gnClientId}}</dd><dt>Client Tag</dt><dd>{{gnClientTag}}</dd><dt>User Id</dt><dd>{{gnUserId}}</dd><dt>Language</dt><dd>English</dd><dt>Country</dt><dd>USA</dd></dl>"></span> 
  			</div>	  				
  		</div>
  		
		{{#if_cond searchType '==' "music"}}
		<div class="row" id='searchbar'>  		
			<div class='col-sm-10'>
			  <div class="col-sm-3"><input type='text' class="form-control search-input copy" placeholder='Artist' id ='artist' value='{{searchParams.[0]}}'></div>
			  <div class="col-sm-3"><input type='text' class="form-control search-input copy" placeholder='Album' id ='album' value='{{searchParams.[1]}}'></div>
			  <div class="col-sm-3"><input type='text' class="form-control search-input copy" placeholder='Track' id ='track' value='{{searchParams.[2]}}'></div>
			  <div class="col-sm-3"><input type='text' class="form-control search-input copy" placeholder='GNID' id ='gnid' value='{{searchParams.[3]}}'></div>
			</div>
			<div class='col-sm-2'>
				<div class="btn btn-default" id='search-button' aria-label="Left Align">
				<span class="glyphicon glyphicon-search" aria-hidden="true"></span> Search</div>				
			</div>	
		</div>
		{{/if_cond}}
	

		{{#if_cond searchType '==' "video"}}
		<div class="row" id='searchbar-video'>  
			<div class="col-md-12">			 	
			    <div class="input-group">
			    	<div class="input-group-btn">			    		
						  <div class="btn btn-default dropdown-toggle" type="button" id='search-type-dropdown' data-toggle="dropdown" aria-expanded="false">
						    <span id='search-type-value'>
						    {{#if searchParams.[0]}}
						    	{{searchParams.[0]}}
						    {{else}}
						    	Search Type
						    {{/if}}
						    </span> 
						    <span class="caret"></span>
						  </div>
						  <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1" >
						    <li role="presentation" class='search-type'>Work Title</li>
						    <li role="presentation" class='search-type'>Series Title</li>				    
						    <li role="presentation" class='search-type'>Contributor Name</li>
						    <li role="presentation" class="divider"></li>
							<li role="presentation" class='search-type'>Work GNID</li>
						    <li role="presentation" class='search-type'>Series GNID</li>				    
						    <li role="presentation" class='search-type'>Season GNID</li>
						    <li role="presentation" class='search-type'>Contributor GNID</li>
						    <li role="presentation" class='search-type'>Disc GNID</li>			
						  </ul>
					</div>			    	
			      
			     	<input type="text" class="form-control" id='video-search-input' value="{{searchParams.[1]}}" placeholder="Choose a search type...">
			      
			      	<div class="input-group-btn">
			        	<div class="btn btn-default" id='search-button-video' type="button"><span class="glyphicon glyphicon-search" aria-hidden="true"></span></div>
			        	
			        	<div class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false" type="button">{{gnResponse.0.range_start}} - {{gnResponse.0.range_end}} of {{gnResponse.0.range_count}} total</div>
			      		<ul class="dropdown-menu dropdown-menu-right" role="menu" aria-labelledby="dropdownMenu2">
			      			<li role="presentation" class="dropdown-header">Result Range</li>
						    <li role="presentation"><a role="menuitem" tabindex="-1" href="0">1 - 10</a></li>
							{{#if_cond gnResponse.0.range_count '>' 10}}
						   		<li role="presentation"><a role="menuitem" tabindex="-1" href="10">10 - 20</a></li>
						    {{/if_cond}}
						    {{#if_cond gnResponse.0.range_count '>' 20}}
						    <li role="presentation"><a role="menuitem" tabindex="-1" href="20">20 - 30</a></li>
						    {{/if_cond}}
						    {{#if_cond gnResponse.0.range_count '>' 30}}
						    <li role="presentation"><a role="menuitem" tabindex="-1" href="30">30 - 40</a></li>
						    {{/if_cond}}
						    {{#if_cond gnResponse.0.range_count '>' 40}}
						    <li role="presentation"><a role="menuitem" tabindex="-1" href="40">40 - 50</a></li>
						    {{/if_cond}}
						    {{#if_cond gnResponse.0.range_count '>' 50}}
						    <li role="presentation"><a role="menuitem" tabindex="-1" href="50">50 - 60</a></li>
						    {{/if_cond}}
						    {{#if_cond gnResponse.0.range_count '>' 60}}
						    <li role="presentation"><a role="menuitem" tabindex="-1" href="60">60 - 70</a></li>
						    {{/if_cond}}
						    {{#if_cond gnResponse.0.range_count '>' 70}}
						    <li role="presentation"><a role="menuitem" tabindex="-1" href="70">70 - 80</a></li>
						    {{/if_cond}}
						    {{#if_cond gnResponse.0.range_count '>' 80}}
						    <li role="presentation"><a role="menuitem" tabindex="-1" href="80">80 - 90</a></li>
						    {{/if_cond}}
						    {{#if_cond gnResponse.0.range_count '>' 90}}
						    <li role="presentation"><a role="menuitem" tabindex="-1" href="90">90 - 100</a></li>
						    {{/if_cond}}
						  </ul>
			      	</div>
			      
			    </div><!-- /input-group -->
		 	</div>
		</div>
		{{/if_cond}}		
		
		
	</div>
</div>